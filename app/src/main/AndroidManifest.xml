<?xml version="1.0" encoding="utf-8"?>
<manifest package="org.ahivs.jetpacking.app"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".basic.BasicActivity"
            android:exported="true" />

        <activity
            android:name=".basic.MessageActivity"
            android:exported="true" />
        <activity
            android:name=".simplelayout.ListActivity"
            android:exported="true" />
        <activity
            android:name=".simplelayout.ScaffoldActivity"
            android:exported="true" />

        <activity
            android:name=".deputy.ListActivity"
            android:exported="true"
            android:theme="@style/DsTheme.NoActionBar"/>

        <activity
            android:name=".deputy.ApproveScreenActivity"
            android:exported="true"
            android:theme="@style/DsTheme.NoActionBar.Transparent" />

    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>